{% set image   = model.image %}
{% set mobileImage = model.mobileImage %}
{% set gravityForm = model.gravityForm %}
{% set video = model.video %}
{% set embeds = model.embed %}
{% set options = options is defined ? options : model.options %}
{% set hubspotForm = model.hubspotForm %}
{% set calendly = model.calendly %}

{% if image %}
	{% include '@Components/Common/_image.twig' with {
    model: image,
    size: 'large',
    class: 'hidden md:block'
  } only %}
	{% include '@Components/Common/_image.twig' with {
    model: mobileImage,
    size: 'medium',
    class: 'md:hidden block max-w-sm mx-auto'
  } only %}
{% endif %}
{% if gravityForm %}
	{% include '@Components/Common/_form.twig' with { model: {gravityForm: gravityForm}, options: options } only %}
{% endif %}
{% if hubspotForm %}
	{% if hubspotForm.editor is defined and hubspotForm.editor == 'new' %}
		{% include '@Components/BlockHubspotFormNew/index.twig' with { model: hubspotForm } only %}
	{% else %}
		{% include '@Components/BlockHubspotFormOld/index.twig' with { model: hubspotForm, options: options } only %}
	{% endif %}
{% endif %}
{% if video %}
	{% include '@Components/BlockVideoOembed/index.twig' with { model: video } only %}
{% endif %}
{% if embeds %}
	{{ embeds }}
{% endif %}
{% if calendly %}
	{% include '@Components/BlockCalendlyEmbed/index.twig' with { model: calendly } only %}
{% endif %}
