{
  "@context": "https://schema.org",
  "@graph": [
    {
      "@type": "WebPage",
      "@id": "{{ PAGE_URL }}",
      "url": "{{ PAGE_URL }}",
      "name": "{{ PAGE_TITLE }}",
      "description": "{{ PAGE_DESCRIPTION }}",
      "isPartOf": {
        "@type": "WebSite",
        "@id": "https://synkedup.com/#website"
      },
      "breadcrumb": {
        "@type": "BreadcrumbList",
        "@id": "{{ PAGE_URL }}#breadcrumb"
      }
    },
    {
      "@type": "LocalBusiness",
      "@id": "{{ PAGE_URL }}#partner",
      "name": "{{ PAGE_TITLE }}",
      "description": "{{ PAGE_DESCRIPTION }}",
      "image": "{{ FEATURE_IMAGE }}"
    },
    {
      "@type": "Organization",
      "@id": "https://synkedup.com/#organization",
      "name": "SynkedUP",
      "url": "https://synkedup.com",
      "logo": {
        "@type": "ImageObject",
        "url": "https://synkedup.com/wp-content/themes/synkedup-theme/dist/assets/logo-light-CpliuxoQ.svg",
        "width": 112,
        "height": 32
      },
      "description": "Landscape business management software built for contractors, by contractors. We help landscape contractors price their jobs profitably. Estimate, win, and track jobs with confidence.",
      "foundingDate": "2020",
      "address": {
        "@type": "PostalAddress",
        "streetAddress": "8583 Woodbury Pike",
        "addressLocality": "Hollidaysburg",
        "addressRegion": "PA",
        "postalCode": "16648",
        "addressCountry": "US"
      },
      "contactPoint": {
        "@type": "ContactPoint",
        "telephone": "+1-814-383-1900",
        "contactType": "customer service"
      },
      "sameAs": [
        "https://www.linkedin.com/company/synkedup/",
        "https://www.facebook.com/synkedup/",
        "https://www.instagram.com/synkedup/",
        "https://www.youtube.com/@synkedup"
      ]
    },
    {
      "@type": "BreadcrumbList",
      "@id": "{{ PAGE_URL }}#breadcrumb",
      "itemListElement": [
        {
          "@type": "ListItem",
          "position": 1,
          "name": "Home",
          "item": "https://synkedup.com"
        },
        {
          "@type": "ListItem",
          "position": 2,
          "name": "Partners",
          "item": "https://synkedup.com/partners/"
        },
        {
          "@type": "ListItem",
          "position": 3,
          "name": "{{ PAGE_TITLE }}",
          "item": "{{ PAGE_URL }}"
        }
      ]
    },
    {
      "@type": "WebSite",
      "@id": "https://synkedup.com/#website",
      "url": "https://synkedup.com/",
      "name": "SynkedUP",
      "description": "Landscape business management software built for contractors, by contractors. We help landscape contractors price their jobs profitably. Estimate, win, and track jobs with confidence.",
      "publisher": {
        "@type": "Organization",
        "@id": "https://synkedup.com/#organization"
      }
    }
  ]
}